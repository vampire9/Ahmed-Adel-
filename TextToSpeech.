import pyttsx3
import PyPDF2
book = open('beauty_and_the_beast.pdf','rb')
pdfReader = PyPDF2.PdfFileReader(book)
pages = pdfReader.numPages
print (pages)
Speaker = pyttsx3.init()
for num in range(0, 10):
  page = pdfReader.getPage(num)
  text = page.extractText()
  Speaker.say(text)
  Speaker.runAndWait()
